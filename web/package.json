{
  "name": "@dexcom-tracker/web",
  "version": "1.0.0",
  "description": "Web application for Dexcom Sensor Tracker",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch --config jest.config.cjs",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true npm run build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser npm run build",
    "perf:test": "node scripts/performance-test.js",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > src/lib/database.types.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "admin:setup": "node ../../scripts/setup-admin.js",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "food:update": "node scripts/update-food-items.js",
    "food:update-all": "node scripts/update-food-items.js --all",
    "food:update-dry-run": "node scripts/update-food-items.js --dry-run"
  },
  "dependencies": {
    "@dexcom-tracker/shared": "file:../shared",
    "@heroicons/react": "^2.2.0",
    "@netlify/functions": "^4.2.7",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.544.0",
    "next": "16.0.0",
    "openfoodfacts-nodejs": "^0.1.0",
    "postcss": "^8.5.6",
    "react": "19.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.2.0",
    "swr": "^2.3.6",
    "tesseract.js": "^6.0.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@netlify/plugin-nextjs": "^5.13.4",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "eslint-config-next": "16.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "supabase": "^2.48.3",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2"
  }
}
