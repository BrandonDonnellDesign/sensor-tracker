{
  "name": "@dexcom-tracker/web",
  "version": "1.0.0",
  "description": "Web application for Dexcom Sensor Tracker",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next build",
    "start": "next start",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch --config jest.config.cjs",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true npm run build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser npm run build",
    "test:web-vitals": "node scripts/test-web-vitals.js",
    "perf:test": "node scripts/performance-test.js",
    "perf:budget": "node scripts/performance-budget.js",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > src/lib/database.types.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "admin:setup": "node ../../scripts/setup-admin.js",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "food:update": "node scripts/update-food-items.js",
    "food:update-all": "node scripts/update-food-items.js --all",
    "food:update-dry-run": "node scripts/update-food-items.js --dry-run"
  },
  "dependencies": {
    "@dexcom-tracker/shared": "file:../shared",
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@netlify/functions": "^5.1.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "^10.29.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.86.2",
    "@types/adm-zip": "^0.5.7",
    "@types/jsonwebtoken": "^9.0.10",
    "adm-zip": "^0.5.16",
    "autoprefixer": "^10.4.22",
    "bcryptjs": "^3.0.3",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.25",
    "googleapis": "^167.0.0",
    "jsonwebtoken": "^9.0.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.555.0",
    "next": "16.0.10",
    "postcss": "^8.5.6",
    "react": "19.2.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "19.2.1",
    "react-is": "^19.2.1",
    "recharts": "^3.5.1",
    "sonner": "^2.0.7",
    "swagger-ui-react": "^5.30.3",
    "swr": "^2.3.7",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.17",
    "tesseract.js": "^6.0.1",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@netlify/plugin-nextjs": "^5.15.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jszip": "^3.4.1",
    "@types/node": "^24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/swagger-ui-react": "^5.18.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "baseline-browser-mapping": "^2.9.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "supabase": "^2.65.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  }
}
